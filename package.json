{
  "name": "pal-test",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "description": "teste de integracao com pal.js",
  "scripts": {
    "start": "node dist/server",
    "clean": "rm -rf dist",
    "build": "npm -s run clean && npm -s run generate && tsc",
    "generate": "npm -s run generate:prisma && npm -s run generate:nexus",
    "generate:prisma": "prisma generate",
    "generate:nexus": "ts-node --transpile-only src/nexusSchema",
    "create-migration": "prisma migrate dev --preview-feature",
    "migrate-database": "prisma db push --preview-feature",
    "dev": "ts-node-dev --no-notify --respawn --transpile-only src/server"
  },
  "dependencies": {
    "nexus": "1.0.0",
    "@prisma/client": "2.19.0",
    "@paljs/nexus": "2.13.0",
    "apollo-server": "2.21.2",
    "graphql": "15.5.0"
  },
  "devDependencies": {
    "prisma": "2.19.0",
    "@types/node": "14.14.35",
    "prettier": "2.2.1",
    "ts-node": "8.10.2",
    "ts-node-dev": "1.1.6",
    "typescript": "4.2.3"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "trailingComma": "all"
  },
  "repository": "https://github.com/tigosante/pal-js_test.git",
  "author": "Tiago Silva"
}
